/**
 * Mai Nou Yang & Nathan Moder
 * 4-way or 4-to-1 multiplexer:
 * out = a if sel=00
 *       b if sel=01
 *       c if sel=10
 *       d if sel=11
 */

CHIP Mux4Way {

    IN a, b, c, d, sel[2];
    OUT out;

    PARTS:
        Mux(a=a, b=b, sel=sel[0], out=out1);
        Mux(a=c, b=d, sel=sel[0], out=out2);
        Mux(a=out1, b=out2, sel=sel[1], out=out);

}
