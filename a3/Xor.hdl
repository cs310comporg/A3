/*
 * Mai Nou Yang & Nathan Moder
 *  Exclusive-or gate: out = !(a == b)
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
        Not(in=a, out=notA);
        Not(in=b, out=notB);
        And(a=notA, b=b, out=out1);
        And(a=a, b=notB, out=out2);
        Or(a=out1, b=out2, out=out);
}
